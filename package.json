{
	"name": "jambot",
	"version": "2.10.0",
	"description": "Jammy bot",
	"main": "built/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"build": "pnpm tsc",
		"dev": "pnpm build && pnpm start",
		"change": "node devScripts/addChange.js",
		"release:minor": "pnpm release --release-as minor",
		"release:patch": "pnpm release --release-as patch",
		"release:major": "pnpm release --release-as major",
		"release": "pnpm standard-version -i docs/changelog.md",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaminitbot/Jam-Bot.git"
	},
	"author": "James Cook",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/jaminitbot/Jam-Bot/issues"
	},
	"homepage": "https://github.com/jaminitbot/Jam-Bot#readme",
	"dependencies": {
		"@discordjs/builders": "^0.7.0",
		"@discordjs/rest": "^0.1.0-canary.0",
		"@sentry/integrations": "^6.13.3",
		"@sentry/node": "^6.13.3",
		"@sentry/tracing": "^6.13.3",
		"dayjs": "^1.10.7",
		"discord-api-types": "^0.24.0",
		"discord.js": "^13.2.0",
		"husky": "^7.0.4",
		"i18next": "^21.3.3",
		"i18next-fs-backend": "^1.1.1",
		"is-image-url": "^1.1.8",
		"is-number": "^7.0.0",
		"lint-staged": "^11.2.6",
		"mongodb": "^4.1.3",
		"ms": "^2.1.3",
		"node-cache": "^5.1.2",
		"node-schedule": "^2.0.0",
		"sha1": "^1.1.1",
		"tslib": "^2.3.1",
		"undici": "^4.8.1",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@types/i18next-fs-backend": "^1.1.2",
		"@types/is-number": "^7.0.1",
		"@types/ms": "^0.7.31",
		"@types/node": "^16.11.4",
		"@types/node-schedule": "^1.3.2",
		"@types/sha1": "^1.1.3",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"dotenv": "^10.0.0",
		"eslint": "^8.1.0",
		"eslint-config-prettier": "^8.3.0",
		"prettier": "^2.4.1",
		"standard-version": "^9.3.2",
		"typescript": "^4.4.4"
	},
	"optionalDependencies": {
		"bufferutil": "^4.0.5",
		"erlpack": "github:discord/erlpack",
		"libsodium-wrappers": "^0.7.9",
		"utf-8-validate": "^5.0.7",
		"zlib-sync": "^0.1.7"
	},
	"engineStrict": true,
	"engines": {
		"node": "16.x"
	},
	"lint-staged": {
		"*.{js,ts}": "eslint --cache --fix",
		"*.{js,css,md,ts}": "prettier --write"
	}
}
